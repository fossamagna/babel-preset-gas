{
  "name": "babel-preset-gas",
  "version": "2.0.4",
  "description": "Babel preset for all Goolge Apps Script plugins.",
  "main": "lib/index.js",
  "scripts": {
    "pretest": "babel --out-dir lib src",
    "test": "eslint src && jest",
    "prepare": "babel --out-dir lib src"
  },
  "keywords": [
    "babel-preset",
    "Google Apps Script"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fossamagna/babel-preset-gas.git"
  },
  "author": "fossamagna <fossamagna2@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fossamagna/babel-preset-gas/issues"
  },
  "homepage": "https://github.com/fossamagna/babel-preset-gas#readme",
  "dependencies": {
    "@babel/helper-plugin-utils": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/plugin-transform-member-expression-literals": "^7.0.0",
    "@babel/plugin-transform-property-literals": "^7.0.0"
  },
  "peerDependencies": {
    "@babel/core": "^7.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/helper-plugin-test-runner": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.0.0",
    "eslint": "^6.0.0",
    "jest": "^24.0.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "jest": {
    "testRegex": "./test/.+\\.js$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/test/fixtures/"
    ],
    "testEnvironment": "node"
  }
}
